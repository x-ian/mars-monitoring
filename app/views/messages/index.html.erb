<h1>Listing messages</h1>

<br/>
<%= will_paginate @messages, :container => false %> (<%= page_entries_info @messages %>)
<br/><br/>

<table>
  <tr>
    <th>Probe</th>
    <th>Message type</th>
    <th>Outgoing message count</th>
    <th>Restart count</th>
    <th>Device time</th>
    <th>Device uptime</th>
    <th>Value1</th>
    <th>Value2</th>
    <th>Value3</th>
    <th>Value4</th>
    <th>Value5</th>
    <th>Value6</th>
    <th>Value7</th>
    <th>Value8</th>
    <th>Value9</th>
    <th>Value10</th>
    <th>Value11</th>
    <th>Value12</th>
    <th>Value13</th>
    <th>Value14</th>
    <th>Value15</th>
    <th>Value16</th>
    <th>Server time</th>
    <th>Probe enabled</th>
    <th>Message archived</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @messages.each do |message| %>
  <tr>
    <td><%= message.probe.name unless message.probe.nil? %> (<%= message.probe.location.name unless message.probe.nil? %>)</td>
    <td><%= message.message_type.name unless message.message_type.nil? %></td>
    <td><%= message.outgoing_message_count %></td>
    <td><%= message.restart_count %></td>
    <td><%= message.device_time.strftime '%Y-%m-%d %H:%M:%S' %></td>
    <td><%= message.device_uptime %></td>
    <td><%= message.value1 %></td>
    <td><%= message.value2 %></td>
    <td><%= message.value3 %></td>
    <td><%= message.value4 %></td>
    <td><%= message.value5 %></td>
    <td><%= message.value6 %></td>
    <td><%= message.value7 %></td>
    <td><%= message.value8 %></td>
    <td><%= message.value9 %></td>
    <td><%= message.value10 %></td>
    <td><%= message.value11 %></td>
    <td><%= message.value12 %></td>
    <td><%= message.value13 %></td>
    <td><%= message.value14 %></td>
    <td><%= message.value15 %></td>
    <td><%= message.value16 %></td>
    <td><%= message.server_time.strftime '%Y-%m-%d %H:%M:%S' %></td>
    <td><%= message.probe_enabled %></td>
    <td><%= message.archived %></td>
    <td><%= link_to 'Show', message %></td>
    <td><%= link_to 'Edit', edit_message_path(message) %></td>
    <td><%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br/>
<%= will_paginate @messages, :container => false %> (<%= page_entries_info @messages %>)
<br/><br/>

<%= current_time_zone.html_safe %>

<%= link_to 'New Message', new_message_path %>
