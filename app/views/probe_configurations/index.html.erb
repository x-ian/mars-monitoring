    <td><%= probe_configuration.value12_threshold %></td>
    <td><%= probe_configuration.value13_threshold %></td>
    <td><%= probe_configuration.value14_threshold %></td>
    <td><%= probe_configuration.value15_threshold %></td>
    <td><%= probe_configuration.value16_threshold %></td>
    <td><%= link_to 'Show', probe_configuration %></td>
    <td><%= link_to 'Edit', edit_probe_configuration_path(probe_configuration) %></td>
    <td><%= link_to 'Destroy', probe_configuration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
